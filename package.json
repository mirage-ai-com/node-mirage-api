{
  "name": "mirage-api",
  "description": "Mirage API Node.",
  "version": "2.0.3",
  "homepage": "https://github.com/mirage-ai-com/node-mirage-api",
  "license": "MIT",
  "author": {
    "name": "Valerian Saliou",
    "email": "valerian@valeriansaliou.name",
    "url": "https://valeriansaliou.name/"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/mirage-ai-com/node-mirage-api.git"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/mirage-ai-com/node-mirage-api/blob/master/LICENSE"
    }
  ],
  "main": "dist/mirage.js",
  "types": "dist/mirage.d.ts",
  "exports": {
    ".": {
      "types": "./dist/mirage.d.ts",
      "import": "./dist/mirage.js",
      "require": "./dist/mirage.js"
    }
  },

  "engines": {
    "node": ">= 16.0.0"
  },
  "scripts": {
    "build": "npx tsc && npx tsc-alias && node -e \"const fs=require('fs');const f='dist/mirage.js';const v=require('./package.json').version;fs.writeFileSync(f, fs.readFileSync(f,'utf8').replace(/__PKG_VERSION_PLACEHOLDER__/g,v));\"",
    "test": "eslint lib && npm run build && istanbul cover _mocha",
    "coveralls": "npm run build && istanbul cover _mocha --report lcovonly && cat ./coverage/lcov.info | coveralls"
  },
  "dependencies": {
    "restler": "3.4.0"
  },
  "devDependencies": {
    "@types/node": "22.13.10",
    "@types/restler": "3.1.9",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "tsc-alias": "1.8.16",
    "typescript": "5.9.2",
    "eslint": "9.29.0",
    "eslint-plugin-crisp": "1.1.13",
    "eslint-plugin-jsdoc": "54.1.0",
    "globals": "15.15.0",
    "mocha": "3.5.0",
    "mocha-lcov-reporter": "1.3.0",
    "istanbul": "0.4.5",
    "coveralls": "2.13.1"
  },
  "keywords": [
    "api",
    "integration"
  ]
}
